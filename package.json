{
  "name": "aws-glue-vscode-extension",
  "displayName": "Aws Glue",
  "description": "Update, Trigger & Monitor your Aws Glue Jobs, Crawlers and Triggers",
  "version": "1.0.0",
  "publisher": "NecatiARSLAN",
  "repository": {
    "type": "git",
    "url": "https://github.com/necatiarslan/aws-glue-vscode-extension"
  },
  "icon": "media/aws-glue-logo-extension.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "AWS",
    "Glue",
    "ETL"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GlueTreeView.Refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.Filter",
        "title": "Filter",
        "icon": "$(filter)"
      },
      {
        "command": "GlueTreeView.ShowOnlyFavorite",
        "title": "Show Only Favorite",
        "icon": "$(bookmark)"
      },
      {
        "command": "GlueTreeView.ShowHiddenNodes",
        "title": "Show Hidden Nodes",
        "icon": "$(eye-closed)"
      },
      {
        "command": "GlueTreeView.AddToFav",
        "title": "Fav",
        "icon": "$(star)"
      },
      {
        "command": "GlueTreeView.DeleteFromFav",
        "title": "UnFav",
        "icon": "$(star-empty)"
      },
      {
        "command": "GlueTreeView.HideNode",
        "title": "Hide",
        "icon": "$(eye-closed)"
      },
      {
        "command": "GlueTreeView.UnHideNode",
        "title": "UnHide",
        "icon": "$(eye)"
      },
      {
        "command": "GlueTreeView.AddGlueResource",
        "title": "Add Glue Resource",
        "icon": "$(add)"
      },
      {
        "command": "GlueTreeView.RemoveGlueResource",
        "title": "Remove Glue Resource",
        "icon": "$(trash)"
      },
      {
        "command": "GlueTreeView.Goto",
        "title": "Goto Aws Console",
        "icon": "$(link-external)"
      },
      {
        "command": "GlueTreeView.SelectAwsProfile",
        "title": "Select Aws Profile",
        "icon": "$(account)"
      },
      {
        "command": "GlueTreeView.RunJob",
        "title": "Run Job",
        "icon": "$(run)"
      },
      {
        "command": "GlueTreeView.RunCrawler",
        "title": "Run Crawler",
        "icon": "$(run)"
      },
      {
        "command": "GlueTreeView.ViewLatestLog",
        "title": "Latest Logs",
        "icon": "$(output)"
      },
      {
        "command": "GlueTreeView.RefreshLogs",
        "title": "Refresh Logs",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.RefreshRuns",
        "title": "Refresh Runs",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.ViewLog",
        "title": "View Log",
        "icon": "$(output)"
      },
      {
        "command": "GlueTreeView.PrintResource",
        "title": "Print Resource",
        "icon": "$(debug-print)"
      },
      {
        "command": "GlueTreeView.TestAwsConnection",
        "title": "Test Aws Connection",
        "icon": "$(plug)"
      },
      {
        "command": "GlueTreeView.UpdateAwsEndPoint",
        "title": "Update Aws EndPoint",
        "icon": "$(settings)"
      },
      {
        "command": "GlueTreeView.Donate",
        "title": "Donate",
        "icon": "$(heart)"
      },
      {
        "command": "GlueTreeView.BugAndNewFeature",
        "title": "Bug and New Feature",
        "icon": "$(issues)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "GlueTreeView.ShowOnlyFavorite",
          "when": "view == GlueTreeView",
          "group": "navigation@1"
        },
        {
          "command": "GlueTreeView.ShowHiddenNodes",
          "when": "view == GlueTreeView",
          "group": "navigation@2"
        },
        {
          "command": "GlueTreeView.Filter",
          "when": "view == GlueTreeView",
          "group": "navigation@3"
        },
        {
          "command": "GlueTreeView.AddGlueResource",
          "when": "view == GlueTreeView",
          "group": "navigation@4"
        },
        {
          "command": "GlueTreeView.SelectAwsProfile",
          "when": "view == GlueTreeView",
          "group": "navigation@6"
        },
        {
          "command": "GlueTreeView.Refresh",
          "when": "view == GlueTreeView",
          "group": "navigation@7"
        },
        {
          "command": "GlueTreeView.TestAwsConnection",
          "when": "view == GlueTreeView",
          "group": "other"
        },
        {
          "command": "GlueTreeView.UpdateAwsEndPoint",
          "when": "view == GlueTreeView",
          "group": "other"
        },
        {
          "command": "GlueTreeView.Donate",
          "when": "view == GlueTreeView",
          "group": "other"
        },
        {
          "command": "GlueTreeView.BugAndNewFeature",
          "when": "view == GlueTreeView",
          "group": "other"
        }
      ],
      "view/item/context": [
        {
          "command": "GlueTreeView.AddToFav",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.DeleteFromFav",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.HideNode",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.UnHideNode",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.RemoveGlueResource",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "Glue"
        },
        {
          "command": "GlueTreeView.Goto",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "Glue"
        },
        {
          "command": "GlueTreeView.RunJob",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.RunCrawler",
          "when": "view == GlueTreeView && viewItem == Crawler",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.ViewLatestLog",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler)",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.PrintResource",
          "when": "view == GlueTreeView && (viewItem == Job || viewItem == Crawler || viewItem == Trigger)",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.RefreshLogs",
          "when": "view == GlueTreeView && viewItem == LogGroup",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.RefreshRuns",
          "when": "view == GlueTreeView && viewItem == RunGroup",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.ViewLog",
          "when": "view == GlueTreeView && viewItem == LogStream",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aws-glue-vscode-extension",
          "title": "Aws Glue",
          "icon": "media/aws-glue-logo-activitybar.png"
        }
      ]
    },
    "views": {
      "aws-glue-vscode-extension": [
        {
          "id": "GlueTreeView",
          "name": "Aws Glue",
          "icon": "media/aws-glue-logo-activitybar.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "GlueTreeView",
        "contents": "Please click [+] to add your Glue Resources. \n [Add Glue Resource](command:GlueTreeView.AddGlueResource) \n Leave Resource name empty to get all your Glue resources. You can enter Resource name partially. \n If you can not connect after a while check your Aws Credentials, Network and Proxy settings. \n\n\n\n To Access Aws, you need to configure aws credentials. \n For more detail on Aws Credentials [Click Here](https://docs.aws.amazon.com/cli/latest/userguide/cli-configure-files.html) \n For a Youtube Tutorial [Click Here](https://www.youtube.com/watch?v=SON8sY1iOBU) \n [Update Aws EndPoint](command:GlueTreeView.UpdateAwsEndPoint)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^24.10.2",
    "@types/vscode": "^1.104.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.948.0",
    "@aws-sdk/client-glue": "^3.964.0",
    "@aws-sdk/client-iam": "^3.948.0",
    "@aws-sdk/client-sts": "^3.948.0",
    "@aws-sdk/credential-providers": "^3.948.0",
    "@vscode-elements/elements": "^2.3.1",
    "@vscode/codicons": "^0.0.43",
    "archiver": "^7.0.1",
    "tmp": "^0.2.5"
  }
}
