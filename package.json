{
  "name": "aws-glue-vscode-extension",
  "displayName": "Aws Glue",
  "description": "Update, Trigger & Monitor your Aws Glue Jobs",
  "version": "1.0.0",
  "publisher": "NecatiARSLAN",
  "repository": {
    "type": "git",
    "url": "https://github.com/necatiarslan/aws-glue-vscode-extension"
  },
  "icon": "media/aws-glue-logo-extension.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "AWS",
    "Glue",
    "ETL",
    "AWS Glue",
    "AWS Glue Job",
    "AWS Glue Job Monitor",
    "AWS Glue Job Trigger",
    "AWS Glue Job Update",
    "AWS Glue Job Run",
    "AWS Glue Job Log",
    "AWS Glue Job Resource",
    "AWS Glue Job Favorite",
    "AWS Glue Job Hide",
    "AWS Glue Job UnHide",
    "AWS Glue Job Remove",
    "AWS Glue Job Add",
    "AWS Glue Job Goto",
    "AWS Glue Job Select Aws Profile",
    "AWS Glue Job Refresh",
    "AWS Glue Job Filter",
    "AWS Glue Job Show Only Favorite",
    "AWS Glue Job Show Hidden Nodes",
    "AWS Glue Job Add To Favorite",
    "AWS Glue Job Delete From Favorite",
    "AWS Glue Job Hide Node",
    "AWS Glue Job UnHide Node",
    "AWS Glue Job Remove Glue Job",
    "AWS Glue Job Add Glue Job",
    "AWS Glue Job Goto Aws Console",
    "AWS Glue Job Select Aws Profile",
    "AWS Glue Job Run Job",
    "AWS Glue Job View Latest Log",
    "AWS Glue Job Refresh Logs",
    "AWS Glue Job Refresh Runs",
    "AWS Glue Job View Log",
    "AWS Glue Job Print Job Info",
    "AWS Glue Job Test Aws Connection",
    "AWS Glue Job Update Aws EndPoint",
    "AWS Glue Job Donate",
    "AWS Glue Job Bug And New Feature"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GlueTreeView.Refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.Filter",
        "title": "Filter",
        "icon": "$(filter)"
      },
      {
        "command": "GlueTreeView.ShowOnlyFavorite",
        "title": "Show Only Favorite",
        "icon": "$(bookmark)"
      },
      {
        "command": "GlueTreeView.ShowHiddenNodes",
        "title": "Show Hidden Nodes",
        "icon": "$(eye-closed)"
      },
      {
        "command": "GlueTreeView.AddToFav",
        "title": "Fav",
        "icon": "$(star)"
      },
      {
        "command": "GlueTreeView.DeleteFromFav",
        "title": "UnFav",
        "icon": "$(star-empty)"
      },
      {
        "command": "GlueTreeView.HideNode",
        "title": "Hide",
        "icon": "$(eye-closed)"
      },
      {
        "command": "GlueTreeView.UnHideNode",
        "title": "UnHide",
        "icon": "$(eye)"
      },
      {
        "command": "GlueTreeView.AddGlueJob",
        "title": "Add Glue Job",
        "icon": "$(add)"
      },
      {
        "command": "GlueTreeView.RemoveGlueJob",
        "title": "Remove Glue Job",
        "icon": "$(trash)"
      },
      {
        "command": "GlueTreeView.Goto",
        "title": "Goto Aws Console",
        "icon": "$(link-external)"
      },
      {
        "command": "GlueTreeView.SelectAwsProfile",
        "title": "Select Aws Profile",
        "icon": "$(account)"
      },
      {
        "command": "GlueTreeView.RunJob",
        "title": "Run Job",
        "icon": "$(run)"
      },
      {
        "command": "GlueTreeView.ViewLatestLog",
        "title": "Latest Logs",
        "icon": "$(output)"
      },
      {
        "command": "GlueTreeView.RefreshLogs",
        "title": "Refresh Logs",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.RefreshRuns",
        "title": "Refresh Runs",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.ViewLog",
        "title": "View Log",
        "icon": "$(output)"
      },
      {
        "command": "GlueTreeView.PrintResource",
        "title": "Print Job Info",
        "icon": "$(info)"
      },
      {
        "command": "GlueTreeView.TestAwsConnection",
        "title": "Test Aws Connection",
        "icon": "$(plug)"
      },
      {
        "command": "GlueTreeView.UpdateAwsEndPoint",
        "title": "Update Aws EndPoint",
        "icon": "$(settings)"
      },
      {
        "command": "GlueTreeView.Donate",
        "title": "Donate",
        "icon": "$(heart)"
      },
      {
        "command": "GlueTreeView.RefreshJobInfo",
        "title": "Refresh Info",
        "icon": "$(refresh)"
      },
      {
        "command": "GlueTreeView.BugAndNewFeature",
        "title": "Bug and New Feature",
        "icon": "$(issues)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "GlueTreeView.ShowOnlyFavorite",
          "when": "view == GlueTreeView",
          "group": "navigation@1"
        },
        {
          "command": "GlueTreeView.ShowHiddenNodes",
          "when": "view == GlueTreeView",
          "group": "navigation@2"
        },
        {
          "command": "GlueTreeView.Filter",
          "when": "view == GlueTreeView",
          "group": "navigation@3"
        },
        {
          "command": "GlueTreeView.AddGlueJob",
          "when": "view == GlueTreeView",
          "group": "navigation@4"
        },
        {
          "command": "GlueTreeView.SelectAwsProfile",
          "when": "view == GlueTreeView",
          "group": "navigation@6"
        },
        {
          "command": "GlueTreeView.Refresh",
          "when": "view == GlueTreeView",
          "group": "navigation@7"
        },
        {
          "command": "GlueTreeView.TestAwsConnection",
          "when": "view == GlueTreeView",
          "group": "other"
        },
        {
          "command": "GlueTreeView.UpdateAwsEndPoint",
          "when": "view == GlueTreeView",
          "group": "other"
        },
        {
          "command": "GlueTreeView.Donate",
          "when": "view == GlueTreeView",
          "group": "other"
        },
        {
          "command": "GlueTreeView.BugAndNewFeature",
          "when": "view == GlueTreeView",
          "group": "other"
        }
      ],
      "view/item/context": [
        {
          "command": "GlueTreeView.AddToFav",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.DeleteFromFav",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.HideNode",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.UnHideNode",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "Filter"
        },
        {
          "command": "GlueTreeView.RemoveGlueJob",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "Glue"
        },
        {
          "command": "GlueTreeView.Goto",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "Glue"
        },
        {
          "command": "GlueTreeView.RunJob",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.ViewLatestLog",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.PrintResource",
          "when": "view == GlueTreeView && viewItem == Job",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.RefreshLogs",
          "when": "view == GlueTreeView && viewItem == LogGroup",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.RefreshRuns",
          "when": "view == GlueTreeView && viewItem == RunGroup",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.RefreshJobInfo",
          "when": "view == GlueTreeView && viewItem == Info",
          "group": "inline"
        },
        {
          "command": "GlueTreeView.ViewLog",
          "when": "view == GlueTreeView && viewItem == LogStream",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aws-glue-vscode-extension",
          "title": "Aws Glue",
          "icon": "media/aws-glue-logo-activitybar.png"
        }
      ]
    },
    "views": {
      "aws-glue-vscode-extension": [
        {
          "id": "GlueTreeView",
          "name": "Aws Glue",
          "icon": "media/aws-glue-logo-activitybar.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "GlueTreeView",
        "contents": "Please click [+] to add your Glue Jobs. \n [Add Glue Job](command:GlueTreeView.AddGlueJob) \n Leave Job name empty to get all your Glue Jobs. You can enter Job name partially. \n If you can not connect after a while check your Aws Credentials, Network and Proxy settings. \n\n\n\n To Access Aws, you need to configure aws credentials. \n For more detail on Aws Credentials [Click Here](https://docs.aws.amazon.com/cli/latest/userguide/cli-configure-files.html) \n For a Youtube Tutorial [Click Here](https://www.youtube.com/watch?v=SON8sY1iOBU) \n [Update Aws EndPoint](command:GlueTreeView.UpdateAwsEndPoint)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.104.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.964.0",
    "@aws-sdk/client-glue": "^3.964.0",
    "@aws-sdk/client-iam": "^3.964.0",
    "@aws-sdk/client-sts": "^3.964.0",
    "@aws-sdk/credential-provider-ini": "^3.964.0",
    "@aws-sdk/credential-providers": "^3.964.0",
    "@aws-sdk/types": "^3.957.0",
    "@vscode-elements/elements": "^2.4.0",
    "@vscode/codicons": "^0.0.44",
    "archiver": "^7.0.1",
    "tmp": "^0.2.5"
  }
}
